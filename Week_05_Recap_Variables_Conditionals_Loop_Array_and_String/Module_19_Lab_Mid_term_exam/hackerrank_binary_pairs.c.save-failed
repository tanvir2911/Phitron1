#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int t,n;
    scanf("%d",&t);

    for(int i=0; i<t; i++)
    {
        scanf("%d",&n);
        char s[n];
        for(int i=0; i<=n; i++)
        {
            scanf("%c",&s[i]);
        }
        s[n+1]='\0';
        puts(s);
        s[]="";
    }






    return 0;
}
