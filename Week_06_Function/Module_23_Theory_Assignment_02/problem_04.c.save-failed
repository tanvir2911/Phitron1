#include<stdio.h>

int main()
{
    int n,k,kth_number;
    scanf("%d %d",&n,&k);

    for(int i=2; i<=n; i+=2)
    {
        printf("%d ",i);
        if(i%2==0)
        {
            if(i==n || i==(n-1))
            {
                i=1;
                printf("%d ",i);
            }
        }
    }

    return 0;
}
