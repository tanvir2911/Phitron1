#include<stdio.h>

int main()
{
    int n;
    scanf("%d",&n);
    for(int i=1;i<=n;i++)
    {
        int x;
        scanf("%d",&x);
        if(x>0)
            for(int j=x;j>=-x;j--) printf("%d ",j);
        else if(x<0)
            for(int j=x;j<=abs(x);j++) printf("%d ",j);
        else printf("%d ",0);
    }

    return 0;
}
